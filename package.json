    "babel-jest": "^23.6.0",
    "babel-runtime": "^6.26.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "jest": "23.6.0",
    "parcel": "1.10.3",
    "parcel-bundler": "1.10.3"
  },
  "now": {
    "name": "flicksy",
